{
  "name": "netlify-plugin-cloudinary",
  "version": "1.6.0",
  "description": "Supercharge images on your Netlify site with Cloudinary!",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest -w",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "compile": "tsc -p ./tsconfig.json",
    "build": "npm run compile && netlify build",
    "deploy": "npm run compile && netlify deploy --build",
    "lint": "eslint . --ext .ts",
    "format": "prettier  --write \"(src|test)/*.+(js|ts|json)\""
  },
  "author": {
    "name": "Colby Fayock",
    "email": "hello@colbyfayock.com",
    "url": "https://twitter.com/colbyfayock"
  },
  "license": "MIT",
  "dependencies": {
    "cloudinary": "^1.40.0",
    "fs-extra": "11.1.1",
    "glob": "^10.3.3",
    "jsdom": "21",
    "node-fetch": "2"
  },
  "devDependencies": {
    "@netlify/config": "20.8.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/fs-extra": "11.0.1",
    "@types/glob": "8.1.0",
    "@types/jsdom": "21.1.2",
    "@types/node-fetch": "2.6.4",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "dotenv": "^16.3.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "39.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.0.2",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.6.4",
    "prettier": "3.0.3",
    "semantic-release": "21.1.1",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  }
}
